    "def cp(x,y): \n",
    "    if (x == 0 or y == 0): \n",
    "        return 1\n",
    "    return (cp(x-1, y) + cp(x, y-1)) \n",
    "\n",
    "print(\"Enter the coordinates: \")\n",
    "x, y = int(input()),int(input())\n",
    "print(\"Paths: \", cp(x, y)) \n",
    "  \n"
